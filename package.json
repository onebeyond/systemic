{
  "name": "systemic",
  "version": "4.1.1",
  "description": "A minimal dependency injection library for node",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "zUnit",
    "coverage": "nyc --report html --reporter lcov --reporter text-summary zUnit",
    "prepare": "husky install",
    "release:prerelease": "npm run release -- prerelease"
  },
  "keywords": [
    "dependency",
    "injection",
    "context",
    "inversion of control",
    "graceful",
    "start up",
    "shutdown",
    "ioc",
    "boot"
  ],
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "nyc": "^15.1.0",
    "prettier": "2.5.1",
    "zunit": "^3.2.1"
  },
  "dependencies": {
    "async": "^3.2.3",
    "debug": "^4.3.4",
    "require-all": "^3.0.0",
    "toposort-class": "^1.0.1"
  },
  "directories": {
    "example": "examples"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.js": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/systemic.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/systemic/issues"
  },
  "homepage": "https://guidesmiths.github.io/systemic/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run qa"
    }
  },
  "zUnit": {
    "pollute": true
  }
}
