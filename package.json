{
  "name": "systemic",
  "version": "4.0.2",
  "description": "A minimal dependency injection library for node",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "qa": "npm run lint && npm test",
    "lint": "lint-staged .",
    "test": "node test",
    "coverage": "nyc --report html --reporter lcov --reporter text-summary node test",
    "prepare": "husky install",
    "release": "standard-version",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "release:prerelease": "npm run release -- prerelease"
  },
  "keywords": [
    "dependency",
    "injection",
    "context",
    "inversion of control",
    "graceful",
    "start up",
    "shutdown",
    "ioc",
    "boot"
  ],
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "devDependencies": {
    "debug": "^4.3.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "nyc": "^15.1.0",
    "prettier": "2.5.1",
    "zunit": "^3.2.1"
  },
  "dependencies": {
    "async": "^3.2.3",
    "require-all": "^3.0.0",
    "standard-version": "^9.5.0",
    "toposort-class": "^1.0.1"
  },
  "directories": {
    "example": "examples"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/systemic.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/systemic/issues"
  },
  "homepage": "https://guidesmiths.github.io/systemic/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run qa"
    }
  }
}
